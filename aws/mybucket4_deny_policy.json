{
	"Version": "2012-10-17",
	"Statement": [
        {
			"Effect": "Deny",
			"Principal": "*",
			"Action": "s3:*",
			"Resource": [
                "arn:aws:s3:::${BUCKET_FOUR}/*",
				"arn:aws:s3:::${BUCKET_FOUR}"
			],
			"Condition":{
				"StringNotLike":{
					"aws:userId":[
						"${ROLE_A_ID}:*",
						"${AWS_ACCOUNT_ID}",
						"${USER_ID}"
					]
				}
			}
		}
	]
}