{
	"Version": "2012-10-17",
	"Statement": [

		{
			"Effect": "Allow",
			"Principal": {
				"AWS": [
                    "${ROLE_A_ARN}"
                ]
			},
			"Action": [  
                "s3:ListBucket"
            ],
			"Resource": [
                "arn:aws:s3:::${BUCKET_FIVE}/*",
				"arn:aws:s3:::${BUCKET_FIVE}"
			]
		},
		{
			"Effect": "Allow",
			"Principal": {
				"AWS": [
                    "${ROLE_B_ARN}"
                ]
			},
			"Action": [ 
                "s3:GetObject"
            ],
			"Resource": [
                "arn:aws:s3:::${BUCKET_FIVE}/*",
				"arn:aws:s3:::${BUCKET_FIVE}"
			]
		}
	]
}